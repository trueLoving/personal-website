<div class="pattern-center-blank">
</div>
<div class="pattern-center ">
  <div class="pattern-attachment-img">
    <img src="<%= page.photos %>" src="<%- theme.lazyloadImg %>" data-src="<%= page.photos %>" class="lazyload"
      onerror="imgError(this,3)" style="width: 100%; height: 100%; object-fit: cover; pointer-events: none;">
  </div>
  <header class="pattern-header ">
    <h1 class="entry-title">文档 链接</h1>
  </header>
</div>
<div id="content" class="site-content">
  <article class="post-item post-301 page type-page status-publish has-post-thumbnail hentry">
    <% if (page.links) { %>
    <div class="links">
      <% (page.links||[]).forEach(function (group, index) { %>
      <h3 class="link-title"><span class="fake-title"><%- index + 1 %>. <%- group.group %></span></h3>
      <% if (group.desc) { %>
      <div class="link-description" style="margin: 10px 0;"><%- group.desc %></div>
      <% } %>
      <ul class="link-items fontSmooth">
        <% (group.items||[]).forEach(function (item) { %>
        <li class="link-item" style="width: 100%;">
          <a class="link-item-inner effect-apollo" href="<%- item.url %>" title="<%- item.desc %>" target="_blank"
            rel="friend">
            <img class="lazyload" style="border-radius: 6px;" onerror="imgError(this,1)" src="<%- theme.lazyloadImg%>" data-src="<%- item.img %>"
              src="<%- item.img %>">
            <span class="sitename">
              <%- item.name %></span>
            <div class="linkdes">
              <%- item.desc %></div>
          </a>
        </li>
        <% }) %>
      </ul>
      <% }) %>
    </div>
    <% } %>
    <div style="text-align:center; width: 100%" class="social-share share-mobile" data-disabled="diandian, tencent">
    </div>
  </article>
  <section class="author-profile">
    <p><i class="iconfont icon-write"></i><%- theme.description%></p>
  </section>
</div>